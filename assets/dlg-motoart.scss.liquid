.new-navigation{
	.link_item_block{
		&:last-child{
			.site-nav__dropdown{
        left: -40%;
        &:before{
          left: 30%;
        }
			}
    }
	}
  .site-nav--has-dropdown{
	  &:hover{
		  .site-nav__dropdown{
        display:flex;
		  }
	  }
	  .site-nav__dropdown{
      background: #1b1b1b;
      display: none;
      top: 46px;
		  &:before{
			  content: "";
			  position: absolute;
        width: 28px;
			  height: 24px;
        background: url({{ "triangle-before.png" | asset_url }});
        top: -17px;
        left: 8%;
        background-size: contain;
		  }
		  ul{
        margin-left:0;padding-left: 10px;
        min-width: 50%;
			  li{
          position:relative;
				  img{
            margin: auto;
					  text-align:center;
				  }
				  &.brand-logo{
					  img{
              max-width: 110px;
            }
				  }
				  &.brand-item-list{
					  img{
              width: 100%;
					  }
				  }
				  a{
            background: transparent;
            color: #FFF;
            text-align: left;
            margin: 0 !important;
            padding-top: 0;
            padding-bottom: 0;
            width: 80%;
            text-decoration:none;
					  &.parent_first_link{
              font-family: 'Open Sans Bold';
              font-size: 15px;
              padding: 3px 10px !important;
            }
					  &.parent_second_link{
              font-family: 'Open Sans Semibold';
              font-size: 13px;
              padding: 0 10px !important;
                line-height: 23px;
            }
          }
			  }
			  &.brand-image-container{
          padding-top: 10px;
				  li{
            text-align: center;
				  }
			  }
			  &.menu-parent-global{
				  .link-parent-global{
					  a{
              border-top: 1px solid transparent;
              border-bottom: 1px solid transparent;
              transition: all 1s;
/*               &:hover{
                border-top: 1px solid $color-red;
                border-bottom: 1px solid $color-red;
              } */
					  }
					  &:last-child{
              margin-top: 40px;
						  a{
                font-family: 'Open Sans SemiboldItalic';
              }
					  }
				  }
			  }
		  }
		  .dropdown-child-link{
			  li{
				  a{
            font-family: 'Open Sans Light Italic';
            font-size: 13px;
            text-transform: capitalize !important;
            padding: 0 10px !important;
				  }
			  }
		  }
	  }
  }
  .menu-Accesorios, .menu-Equipamiento{
    padding: 0 !important;
	  .menu-parent-global{
      padding: 15px !important;
		  li{
			  &.link-parent-global{
          border-top: 1px solid transparent;
          border-bottom: 1px solid transparent;
          transition: all 1s;
          &:hover{
            border-top: 1px solid $color-red;
            border-bottom: 1px solid $color-red;
          }
			  }
		  }
	  }
    .brand-image-container{
      padding: 0 !important;
      margin: 0 !important;
      .brand-logo{
        img{
          width: 100%;
          max-width: 100% !important;
        }
      }
    }
  }
  .collapsed-menu{
	  max-height:0;
	  overflow:hidden;
	  transition: max-height 1s cubic-bezier(0, 1, 0, 1);
	  &.expanded{
		  max-height:600px;
		  transition: max-height 1s ease-in-out;
	  }
  }
  .site-nav__deep-dropdown-trigger {
    .icon-chevron-down{
      position: absolute;
      top: 13px;
      color: white;
      left: 0px;
	    &.opened{
        transform: rotate(0deg) translateX(50%);
           top: 8px;
	    }
    }
    svg{
	    &.icon{
        cursor:pointer;
	    }
    }
  }
