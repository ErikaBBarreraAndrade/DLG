$color-red: #F00000;
/*================== DLG Header ==================*/
.header-item.header-item--logo{
  @media(min-width: 992px){
    padding-bottom: 20px;
  }
}
body{
  overflow-x: hidden !important;
}
header{
  .header-item--icons{
    @media(min-width: 992px){
      padding-bottom: 25px;
    }
  }
}
.header-black .header-sticky-wrapper{
  background: #000 !important;
}
.header-black .site-header--opening{
  background: #000 !important;
}
.header-black .main-content{
  margin-top: 100px;
}
.header-wrapper--sticky .site-header:not(.site-header--stuck){
  background: rgba(0,0,0,.27) !important;
}
.site-header--opening{
  background: #000 !important;
}
.slider-products{
  .grid-product__color-image{
    position: relative !important;
  }
}
.product-slider-home{
  .grid-product__color-image{
    position: relative !important;
  }
  .block-dropdown-variant{
    display: none;
  }
}
.header-dlg{
  position: relative;
  z-index: 999;
  header{
    .site-nav{
      margin-left: auto !important;
      @media(min-width: 992px){
        position: relative;
        z-index: 9;
      }
      .site-nav__item{
        &.site-nav--has-dropdown{
          .site-nav__dropdown{
            padding: 10px 10px 24px;
            width: 445px;
            min-height: 450px;
            li{
              a{
                margin-left: 15px;
                padding-left: 10px;
                text-transform: uppercase;
                transition: all 1s;
                padding-left: 25px !important;
                &:hover{
                  color: $color-red;
                }
              }
            }
          }
          &.Motos{
            .site-nav__dropdown{
              background: url({{ 'menu-motos.png' | asset_url }}) no-repeat, bottom;
              background-size: cover;
              width: 471px;
              background-position: bottom;
              li{
                a{
                  font-family: 'Open Sans Bold';
                  background-color: transparent !important;
                  font-size: 15px;
                  line-height: 15px;
                  color: #FFF;
                  text-decoration: none;
                  @media(min-width: 768px){
                    font-size: 15px;
                    line-height: 15px;
                  }
                  @media(min-width: 992px){
                    font-size: 15px;
                    line-height: 15px;
                  }
                }
              }
            }
          }
        }
        .site-nav__link{
          font-family: 'Open Sans Regular';
          letter-spacing: 0;
          color: #FFF !important;
          background: transparent !important;
          @media(min-width: 992px){
            padding: 7.5px 25px;
            padding-bottom: 25px;
          }
        }
      }
    }
  }
}
