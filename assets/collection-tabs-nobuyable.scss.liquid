$color-red: #FF0000;
.page_magni{
	#myTab{
    display: none;
	}
  .content-card-motos{
    border-top: 1px solid $color-red;
    padding: 50px 0px;
    @media(min-width: 576px){
      padding: 85px 0px;
    }
    @media(min-width: 992px){
      padding: 75px 0px;
    }
    @media(min-width: 1280px){
      padding: 105px 0px;
    }
  }
  .collection-tabs-nobuyable{
    .title-tabs{
      font-family: "Open Sans Regular";
      font-size: 35px;
      line-height: 38px;
      @media(min-width: 576px){
        font-size: 45px;
        line-height: 47px;
      }
      @media(min-width: 992px){
        font-size: 37px;
        line-height: 30px;
      }
      @media(min-width: 1280px){
        font-size: 45px;
        line-height: 38px;
      }
      strong {
        font-family: "Open Sans Bold";
        color: $color-red;
      }
    }
    .subtitle-tabs{
      font-family: "Open Sans Regular";
      font-size: 21px;
      line-height: 27px;
      margin-top: 15px;
      @media (min-width: 576px){
        font-size: 45px;
        line-height: 51px;
      }
      @media(min-width: 992px){
        font-size: 35px;
        line-height: 41px;
        margin-top: 0;
      }
      @media(min-width: 1280px){
        font-size: 45px;
        line-height: 51px;
      }
    }
    .description-tabs{
      font-family: "Open Sans Regular";
      font-size: 13px;
      line-height: 19px;
      margin: auto;
      margin-top: 25px;
      @media(min-width: 576px){
        font-size: 25px;
        line-height: 27px;
        margin-top: 53px;
        width: 510px;
      }
      @media(min-width: 992px){
        margin-top: 25px;
        width: 500px;
        font-size: 14px;
        line-height: 17px;
      }
      @media(min-width: 1280px){
        font-size: 16px;
        line-height: 19px;
      }
    }
    .box-motos-description{
      color: #FFFFFF;
      margin-top: 40px;
      padding: 20px;
      margin-bottom: 30px;
      @media(min-width: 576px){
        margin-top: 40px;
        padding: 30px;
      }
      @media(min-width: 992px){
        margin-top: 90px;
        padding: 30px;
        padding-right: 0px;
      }
      @media(min-width: 1280px){
        margin-top: 90px;
        padding: 0;
        padding-left: 30px;
      }
      .content-description{
        .title{
          font-family: "Open Sans Bold";
          font-size: 25px;
          line-height: 37px;
          @media(min-width: 576px){
            font-size: 35px;
            line-height: 47px;
          }
          @media(min-width: 992px){
            font-size: 25px;
            line-height: 25px;
          }
          @media(min-width: 1280px){
            font-size: 30px;
            line-height: 41px;
          }
        }
        .description-motos{
          font-family: "Open Sans Regular";
          font-size: 13px;
          line-height: 18px;
          margin-bottom: 20px;
          @media(min-width: 576px){
            font-size: 20px;
            line-height: 25px;
            margin-bottom: 25px;
          }
          @media(min-width: 992px){
            font-size: 12px;
            line-height: 15px;
            margin-bottom: 10px;
          }
          @media(min-width: 1280px){
            font-size: 13px;
            line-height: 18px;
            margin-bottom: 10px;
            width: 510px;
          }
        }
        .label{
          font-family: "Open Sans Regular";
          color: #999999;
          text-transform: initial;
          font-size: 10px;
          line-height: 15px;
          @media(min-width: 576px){
            font-size: 15px;
            line-height: 20px;
          }
          @media(min-width: 992px){
            font-size: 11px;
            line-height: 16px;
          }
          @media(min-width: 1280px){
            font-size: 13px;
            line-height: 18px;
          }
        }
        .description-label{
          font-family: "Open Sans Regular";
          font-size: 10px;
          line-height: 15px;
          @media(min-width: 576px){
            font-size: 15px;
            line-height: 21px;
          }
          @media(min-width: 992px){
            font-size: 11px;
            line-height: 16px;
          }
          @media(min-width: 1280px){
            font-size: 13px;
            line-height: 18px;
          }
        }
        ul{
          li{
            &.price{
              font-family: "Open Sans Bold";
              font-size: 18px;
              line-height: 55px;
              @media(min-width: 576px){
                font-size: 25px;
                line-height: 55px;
              }
              @media(min-width: 992px){
                margin-right: 30px;
                font-size: 20px;
                line-height: 50px;
              }
              @media(min-width: 1280px){
                margin-right: 30px;
                font-size: 25px;
                line-height: 55px;
              }
            }
            &.content-button{
              margin-bottom: 30px !important;
              @media(min-width: 576px){
                margin-bottom: 50px !important;
              }
              @media(min-width: 992px){
                margin-bottom: auto !important;
              }
              a{
                font-family: 'Open Sans Condensed Bold';
                color: #FFF;
                background: $color-red;
                font-size: 14px;
                line-height: 14px;
                border-radius: 50px;
                @media(min-width: 576px){
                  font-size: 17px;
                  line-height: 14px;
                }
                @media(min-width: 992px){
                  border-radius: 0;
                  font-size: 17px;
                  line-height: 14px;
                }
              }
            }
          }
        }
      }
    }
  }
}
