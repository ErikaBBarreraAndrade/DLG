		<div id="shopify-section-ducati-motos-nuevas-title-tabs" class="shopify-section agusta-seminuevas-tabs row justify-content-center">
			<div class="col-12">
				<div class="row justify-content-center">
					<div class="col-12">
						{%- assign featured_product = collections[section.settings.featured_product_handle] -%}
						{%- for product in featured_product.products -%}
							{% include 'catalog-card-product-list-alone', cProduct:product %}
						{%- endfor -%}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
